//audioPolicy
AudioPolicyService::onFirstRef{
- AudioPolicyClient(this)
- createAudioPolicyManager(mAudioPolicyClient)-->
	AudioPolicyManager(clientInterface){
	- mVolumeCurves = new VolumeCurvesCollection //音量表
	- AudioPolicyConfig config   //配置
	- serializer.deserialize("audio_policy_configuration.xml", config)//解析配置
	- mVolumeCurves->initializeVolumeCurves//初始化音量表
	- mEngine = engineInstance->queryInterface //获取接口
	- mAvailableOutputDevices //初始化输出设备
 	- mAvailableInputDevices //输入设备 
	}
}




