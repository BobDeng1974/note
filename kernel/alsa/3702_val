i2cset 6 0x40 0xfe 0x81
i2cset 6 0x40 0x30 0x80

i2cset 6 0x40 0x01 0xe7
i2cset 6 0x40 0x02 0x03
i2cset 6 0x40 0x2c 0x80
i2cset 6 0x40 0x90 0x30
i2cset 6 0x40 0x41 0x10
i2cset 6 0x40 0x44 0x11
i2cset 6 0x40 0x47 0x00
i2cset 6 0x40 0x30 0xff
i2cset 6 0x40 0x05 0x0b

i2cset 6 0x40 0x05 0x03



   //reset
i2cset 4 0x40 0xfe  0x81 

i2cset 4 0x40 0x01  0xe7 
i2cset 4 0x40 0x02  0x11 
i2cset 4 0x40 0x03  0x19 
i2cset 4 0x40 0x90  0x30

i2cset 4 0x40 0x05  0x0b 
i2cset 4 0x40 0x06  0x05 

   //volume gain
i2cset 4 0x40 0x20  0x80 
i2cset 4 0x40 0x28  0x80 
i2cset 4 0x40 0x29  0x80  
i2cset 4 0x40 0x2b  0x80 
i2cset 4 0x40 0x2a  0x80 
i2cset 4 0x40 0x2c  0x80 
i2cset 4 0x40 0x30  0x80 

   //bass middle treble
i2cset 4 0x40 0x41  0x10 
i2cset 4 0x40 0x44  0x11 
i2cset 4 0x40 0x47  0x00 
i2cset 4 0x40 0x51  0x04 
i2cset 4 0x40 0x54  0x00 
i2cset 4 0x40 0x57  0x0f 
i2cset 4 0x40 0x75  0x60 
    

dd if=/sdcard/boot.img of=/dev/block/platform/soc/soc:ap-ahb/c0c00000.sdio/by-name/boot bs=1024 count=16000

dd if=/sdcard/u-boot-sign.bin of=/dev/block/platform/soc/soc:ap-ahb/c0c00000.sdio/by-name/uboot bs=1024 count=590


dd if=/storage/USB0/boot.img of=/dev/block/by-name/boot bs=1024 count=24010

